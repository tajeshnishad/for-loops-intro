name = "Tajesh"
for i in name :
  print(i)
  if(i=='j'):
    print("There is something special")

colors = ("red" , "green" , "blue" , "yellow")
for color in colors:
  print(color)
  if(color == "green"):
    print ("these is a green colour " )
  for i in color:
    print(i)


for i in range(1 , 20):
  print(i+1)
